<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0"
xmlns:android="http://schemas.android.com/apk/res/android"
           id="cordova-plugin-GoogleMobileAdsMediationMoPub"
      version="0.1.0">

    <name>cordova-plugin-GoogleMobileAdsMediationMoPub</name>
    <description>Cordova plugin to install the GoogleMobileAdsMediationMoPub pod </description>
    <license>MIT</license>
    <keywords>cordova</keywords>
    <repo>https://github.com/jellomaster/GoogleMobileAdsMediationMoPub</repo>
    <issue>https://github.com/jellomaster/GoogleMobileAdsMediationMoPub/issues</issue>


    <engines>
        <engine name="cordova" version=">=6.0.0"/>
        <engine name="cordova-ios" version=">=4.3.0"/>
    </engines>

    <platform name="ios">
        <framework src="GoogleMobileAdsMediationMoPub" type="podspec" spec="~> 2.0.1"/>
    </platform>

</plugin>